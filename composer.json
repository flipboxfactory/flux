{
  "name": "flipboxfactory/flux",
  "description": "Manipulate data structures",
  "version": "1.0.0",
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "craftcms/cms": "^3.0",
    "flipboxfactory/craft-ember": "^2.0",
    "flipboxdigital/transform": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\flux\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\flux\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "name": "Flux",
    "handle": "flux",
    "class": "flipbox\\flux\\Flux",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/flux/master/CHANGELOG.md",
    "documentationUrl": "https://flux.flipboxfactory.com",
    "schemaVersion": "1.0.1",
    "hasCpSettings": false,
    "hasCpSection": true
  }
}
